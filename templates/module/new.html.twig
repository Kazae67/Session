{% extends 'base.html.twig' %}

{% block title %}New Module{% endblock %}

{% block body %}
    <div class="container">
        <h1>Create new Module</h1>

        {# message d'erreur #}
        {% for message in app.flashes('error') %}
            <div class="alert alert-danger">
                {{ message }}
            </div>
        {% endfor %}

        <div class="form-container">
            {{ include('module/_form.html.twig') }}
        </div>

        <a href="{{ path('app_module_index') }}" class="back-button">Back to list</a>
    </div>
{% endblock %}
