{% extends 'base.html.twig' %}

{% block title %}Session{% endblock %}

{% block body %}
    <h1>Session</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ session.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ session.label }}</td>
            </tr>
            <tr>
                <th>DateStart</th>
                <td>{{ session.dateStart ? session.dateStart|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateEnd</th>
                <td>{{ session.dateEnd ? session.dateEnd|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>NbLimit</th>
                <td>{{ session.nbLimit }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_symfony_console_make_crud_stagiaire_index') }}">back to list</a>

    <a href="{{ path('app_symfony_console_make_crud_stagiaire_edit', {'id': session.id}) }}">edit</a>

    {{ include('symfony_console_make_crud_stagiaire/_delete_form.html.twig') }}
{% endblock %}
